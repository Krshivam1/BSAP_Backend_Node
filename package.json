{
  "name": "performancestatistics-nodejs",
  "version": "1.0.0",
  "description": "Performance Statistics Backend API - Node.js Express with Sequelize ORM",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:force": "node scripts/setup-database.js --force",
    "db:status": "npx sequelize-cli db:migrate:status"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.32.1",
    "mysql2": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^6.8.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "axios": "^1.5.0",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.13.0",
    "csv-parser": "^3.0.0",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "api",
    "performance-statistics"
  ],
  "author": "Your Name",
  "license": "MIT"
}